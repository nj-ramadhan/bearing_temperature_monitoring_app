#:kivy 2.1.0

#:import NoTransition kivy.uix.screenmanager.NoTransition
#:include screen_splash.kv
#:include screen_dashboard.kv
#:include screen_data.kv


# ScreenManager:
<RootScreen>:
    id: screen_manager
    transition: NoTransition()
    
    ScreenSplash:
        app_window: app
        name: 'screen_splash'
        screen_manager: screen_manager
        screen_dashboard: screen_dashboard

    ScreenDashboard:
        id: screen_dashboard
        name: 'screen_dashboard'
        screen_manager: screen_manager

    ScreenData:
        id: screen_data
        name: 'screen_data'
        screen_manager: screen_manager
         

                  
